@model VolunteerList
@{
    ViewBag.Title = "Delete";
}

<h2>Do you want to delete this record?</h2>


@using (Html.BeginForm())
{

    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.VolunteerId)

    <table class="table">
        <tbody>
            <tr>
                <td><b>Name</b></td>
                <td>@Html.DisplayFor(model => model.Fullname)</td>
            </tr>
            <tr>
                <td><b>Region</b></td>
                <td>@Html.DisplayFor(model => model.lib_regions.region_name)</td>
            </tr>
        </tbody>

    </table>

    @Html.ActionLink("Cancel", "Index", "Home", null, new { @class = "btn btn-default float-right btn-flat" })
    <button type="submit" class="btn btn-danger float-right btn-flat">Delete</button>


}